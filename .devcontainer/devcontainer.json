{
  "name": "CSV to XLSX Converter from dev container",
  // "image": "ghcr.io/streamlit/basic-template:latest",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md",
        "csv2xlsx_streamlit_app.py"
      ]
    }
  },
  "postAttachCommand": {
    "server": "streamlit run csv2xlsx_streamlit_app.py --server.enableCORS false --server.enableXsrfProtection false"
  },
  "portsAttributes": {
    "8501": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    8501
  ]
}
